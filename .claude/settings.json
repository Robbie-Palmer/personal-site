{
  "permissions": {
    "allow": [
      "Bash(git status:*)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git show:*)",
      "Bash(git branch --list:*)",
      "Bash(git remote -v:*)",
      "Bash(pwd:*)",
      "Bash(which:*)",
      "Bash(whoami:*)"
    ],
    "deny": [
      "Read(**/.env)",
      "Read(**/.env.*)",
      "Read(**/secrets/**)",
      "Read(**/config/secrets.*)",
      "Read(**/credentials.json)",
      "Read(**/*.pem)",
      "Read(**/*.key)",
      "Read(**/*-key.json)",
      "Read(**/terraform.tfstate)",
      "Read(**/terraform.tfstate.backup)",
      "Read(**/terraform.tfvars)",
      "Read(**/*.tfvars)",
      "Read(**/.aws/credentials)",
      "Read(**/.ssh/id_*)",
      "Read(**/.ssh/authorized_keys)",
      "Read(**/service-account*.json)",
      "Read(**/*-service-account*.json)",
      "Read(**/.npmrc)",
      "Read(**/.docker/config.json)",
      "Edit(**/.env)",
      "Edit(**/.env.*)",
      "Edit(**/secrets/**)",
      "Edit(**/config/secrets.*)",
      "Edit(**/credentials.json)",
      "Edit(**/*.pem)",
      "Edit(**/*.key)",
      "Edit(**/*-key.json)",
      "Edit(**/terraform.tfstate)",
      "Edit(**/terraform.tfstate.backup)",
      "Edit(**/terraform.tfvars)",
      "Edit(**/*.tfvars)",
      "Edit(**/.aws/credentials)",
      "Edit(**/.aws/config)",
      "Edit(**/.ssh/id_*)",
      "Edit(**/.ssh/config)",
      "Edit(**/.ssh/authorized_keys)",
      "Edit(**/service-account*.json)",
      "Edit(**/*-service-account*.json)",
      "Edit(**/.npmrc)",
      "Edit(**/.docker/config.json)"
    ]
  }
}
